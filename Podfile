# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoaPodsDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoaPodsDemo
pod 'Alamofire' # not specifying the version number will install the most recent version of the library (MOST OF THE TIME as per what happened to David) it also supported in swift packager. 

# SnapKit is an AutoLayout third party library this is also supported in swift package manager
pod 'SnapKit'

#Kingfisher is a photo processing library
pod 'Kingfisher'
end
